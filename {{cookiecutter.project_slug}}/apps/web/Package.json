{
  "name": "{{cookiecutter.project_slug}}-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "{% if cookiecutter.frontend_framework == 'nextjs' %}next dev{% else %}vite{% endif %}",
    "build": "{% if cookiecutter.frontend_framework == 'nextjs' %}next build{% else %}vite build{% endif %}",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "dependencies": {
    {% if cookiecutter.frontend_framework == "nextjs" -%}
    "next": "^14.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
    {% else -%}
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^5.0.0"
    {% endif -%}
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "eslint": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "jest": "^29.0.0"
  }
}
